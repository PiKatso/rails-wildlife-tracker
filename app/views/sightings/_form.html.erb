<%= render "layouts/errors", :object => @sighting %>

<%= form_for [@animal, @sighting] do |f| %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :lat %>
  <%= f.text_field :lat %>
  <%= f.label :lng %>
  <%= f.text_field :lng %>
  <%= f.label :region %>
  <%= f.text_field :region %>
  <%= f.submit %>
<% end %>
